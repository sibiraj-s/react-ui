{
  "name": "react-ui",
  "version": "0.0.0",
  "description": "React UI components and utilities",
  "repository": "https://github.com/sibiraj-s/react-ui.git",
  "author": "sibiraj-s",
  "private": true,
  "bugs": "https://github.com/sibiraj-s/react-ui/issues",
  "homepage": "https://github.com/sibiraj-s/react-ui#readme",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.ts",
  "keywords": [
    "react-ui",
    "components",
    "hooks",
    "react-utilities"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "build": "tsup",
    "studio": "panda studio",
    "dev": "storybook dev -p 4004 --no-open",
    "build:storybook": "storybook build",
    "lint": "eslint . --ignore-path=.gitignore --ext=.js,.ts,.tsx",
    "prepare": "husky install && panda codegen"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {
    "@internationalized/date": "^3.5.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@react-aria/button": "^3.9.0",
    "@react-aria/datepicker": "^3.9.0",
    "@react-aria/focus": "^3.15.0",
    "@react-aria/i18n": "^3.9.0",
    "@react-aria/link": "^3.6.2",
    "@react-aria/progress": "^3.4.8",
    "@react-aria/switch": "^3.5.7",
    "@react-aria/textfield": "^3.13.0",
    "@react-aria/utils": "^3.22.0",
    "@react-stately/datepicker": "^3.9.0",
    "@react-stately/toggle": "^3.7.0",
    "framer-motion": "^10.16.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@pandacss/dev": "^0.18.3",
    "@storybook/addon-a11y": "^7.5.3",
    "@storybook/addon-actions": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/manager-api": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/theming": "^7.5.3",
    "@testing-library/react": "^14.1.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitest/coverage-v8": "^0.34.6",
    "commitlint-config-non-conventional": "^1.0.1",
    "eslint": "^8.53.0",
    "eslint-config-pegasus": "^5.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.3",
    "storybook-dark-mode": "^3.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
