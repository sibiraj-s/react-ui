{
  "name": "react-ui",
  "version": "0.0.0",
  "description": "React UI components and utilities",
  "repository": "https://github.com/sibiraj-s/react-ui.git",
  "author": "sibiraj-s",
  "private": true,
  "bugs": "https://github.com/sibiraj-s/react-ui/issues",
  "homepage": "https://github.com/sibiraj-s/react-ui#readme",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.ts",
  "keywords": [
    "react-ui",
    "components",
    "hooks",
    "react-utilities"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "build": "tsup",
    "dev": "storybook dev -p 4004 --no-open",
    "build:storybook": "storybook build",
    "lint": "eslint . --ignore-path=.gitignore --ext=.js,.ts,.tsx",
    "prepare": "husky && panda codegen"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {
    "@internationalized/date": "^3.5.3",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@react-aria/datepicker": "^3.10.0",
    "@react-aria/focus": "^3.17.0",
    "@react-aria/i18n": "^3.11.0",
    "@react-aria/progress": "^3.4.12",
    "@react-aria/switch": "^3.6.3",
    "@react-aria/textfield": "^3.14.4",
    "@react-aria/utils": "^3.24.0",
    "@react-stately/datepicker": "^3.9.3",
    "@react-stately/toggle": "^3.7.3",
    "framer-motion": "^11.1.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@pandacss/dev": "^0.39.1",
    "@storybook/addon-a11y": "^8.0.10",
    "@storybook/addon-actions": "^8.0.10",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/manager-api": "^8.0.10",
    "@storybook/react": "^8.0.10",
    "@storybook/react-vite": "^8.0.10",
    "@storybook/theming": "^8.0.10",
    "@testing-library/react": "^15.0.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^1.6.0",
    "commitlint-config-non-conventional": "^1.0.1",
    "eslint": "^8.57.0",
    "eslint-config-pegasus": "^5.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.0.10",
    "storybook-dark-mode": "^4.0.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  }
}
