{
  "name": "react-ui",
  "version": "0.0.0",
  "description": "React UI components and utilities",
  "repository": "https://github.com/sibiraj-s/react-ui.git",
  "author": "sibiraj-s",
  "private": true,
  "bugs": "https://github.com/sibiraj-s/react-ui/issues",
  "homepage": "https://github.com/sibiraj-s/react-ui#readme",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.ts",
  "keywords": [
    "react-ui",
    "components",
    "hooks",
    "react-utilities"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "build": "tsup",
    "dev": "storybook dev -p 4004 --no-open",
    "build:storybook": "storybook build",
    "lint": "eslint . --ignore-path=.gitignore --ext=.js,.ts,.tsx",
    "prepare": "husky && panda codegen"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "dependencies": {
    "@internationalized/date": "^3.6.0",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-aspect-ratio": "^1.1.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@react-aria/datepicker": "^3.12.0",
    "@react-aria/focus": "^3.19.0",
    "@react-aria/i18n": "^3.12.4",
    "@react-aria/progress": "^3.4.18",
    "@react-aria/switch": "^3.6.10",
    "@react-aria/textfield": "^3.15.0",
    "@react-aria/utils": "^3.26.0",
    "@react-stately/datepicker": "^3.11.0",
    "@react-stately/toggle": "^3.8.0",
    "motion": "^11.16.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@pandacss/dev": "^0.51.1",
    "@pandacss/eslint-plugin": "^0.2.3",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/manager-api": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/theming": "^8.4.7",
    "@testing-library/react": "^16.1.0",
    "@types/react": "19.0.4",
    "@types/react-dom": "^19.0.2",
    "@vitest/coverage-v8": "^2.1.8",
    "commitlint-config-non-conventional": "^1.0.1",
    "eslint": "^8.57.0",
    "eslint-config-pegasus": "^5.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.11.2",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^8.4.7",
    "storybook-dark-mode": "^4.0.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  }
}
