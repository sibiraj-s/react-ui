{
  "name": "react-ui",
  "version": "0.0.0",
  "description": "React UI components and utilities",
  "repository": "https://github.com/sibiraj-s/react-ui.git",
  "author": "sibiraj-s",
  "private": true,
  "bugs": "https://github.com/sibiraj-s/react-ui/issues",
  "homepage": "https://github.com/sibiraj-s/react-ui#readme",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.ts",
  "keywords": [
    "react-ui",
    "components",
    "hooks",
    "react-utilities"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "build": "tsup",
    "dev": "storybook dev -p 4004 --no-open",
    "build:storybook": "storybook build",
    "lint": "eslint . --ignore-path=.gitignore --ext=.js,.ts,.tsx",
    "prepare": "husky && panda codegen"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {
    "@internationalized/date": "^3.5.5",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@react-aria/datepicker": "^3.11.2",
    "@react-aria/focus": "^3.18.2",
    "@react-aria/i18n": "^3.12.2",
    "@react-aria/progress": "^3.4.16",
    "@react-aria/switch": "^3.6.7",
    "@react-aria/textfield": "^3.14.8",
    "@react-aria/utils": "^3.25.2",
    "@react-stately/datepicker": "^3.10.2",
    "@react-stately/toggle": "^3.7.7",
    "framer-motion": "^11.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@pandacss/dev": "^0.46.0",
    "@pandacss/eslint-plugin": "^0.1.11",
    "@storybook/addon-a11y": "^8.3.0",
    "@storybook/addon-actions": "^8.3.0",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/manager-api": "^8.3.0",
    "@storybook/react": "^8.3.0",
    "@storybook/react-vite": "^8.3.0",
    "@storybook/theming": "^8.3.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^2.1.1",
    "commitlint-config-non-conventional": "^1.0.1",
    "eslint": "^8.57.0",
    "eslint-config-pegasus": "^5.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.3.0",
    "storybook-dark-mode": "^4.0.2",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vitest": "^2.1.1"
  }
}
