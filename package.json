{
  "name": "react-ui",
  "version": "0.0.0",
  "description": "React UI components and utilities",
  "repository": "https://github.com/sibiraj-s/react-ui.git",
  "author": "sibiraj-s",
  "private": true,
  "bugs": "https://github.com/sibiraj-s/react-ui/issues",
  "homepage": "https://github.com/sibiraj-s/react-ui#readme",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.ts",
  "keywords": [
    "react-ui",
    "components",
    "hooks",
    "react-utilities"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "build": "tsup",
    "dev": "storybook dev -p 4004 --no-open",
    "build:storybook": "storybook build",
    "lint": "eslint . --ignore-path=.gitignore --ext=.js,.ts,.tsx",
    "prepare": "husky && panda codegen"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "dependencies": {
    "@internationalized/date": "^3.7.0",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@react-aria/datepicker": "^3.14.1",
    "@react-aria/focus": "^3.20.1",
    "@react-aria/i18n": "^3.12.7",
    "@react-aria/progress": "^3.4.21",
    "@react-aria/switch": "^3.7.1",
    "@react-aria/textfield": "^3.17.1",
    "@react-aria/utils": "^3.28.1",
    "@react-stately/datepicker": "^3.13.0",
    "@react-stately/toggle": "^3.8.2",
    "motion": "^12.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@pandacss/dev": "^0.53.2",
    "@pandacss/eslint-plugin": "^0.2.8",
    "@storybook/addon-a11y": "^8.6.8",
    "@storybook/addon-actions": "^8.6.8",
    "@storybook/addon-essentials": "^8.6.8",
    "@storybook/addon-links": "^8.6.8",
    "@storybook/manager-api": "^8.6.8",
    "@storybook/react": "^8.6.8",
    "@storybook/react-vite": "^8.6.8",
    "@storybook/theming": "^8.6.8",
    "@testing-library/react": "^16.2.0",
    "@types/react": "19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitest/coverage-v8": "^3.0.9",
    "commitlint-config-non-conventional": "^1.0.1",
    "eslint": "^8.57.1",
    "eslint-config-pegasus": "^5.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-storybook": "^0.11.6",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^8.6.8",
    "storybook-dark-mode": "^4.0.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vitest": "^3.0.9"
  }
}
